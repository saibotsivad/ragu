{
  "name": "ragu",
  "version": "0.0.0",
  "description": "A tasty little Markdown to HTML site generator with very few opinions.",
  "type": "module",
  "bin": "bin.js",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:lint",
    "test:unit": "uvu src",
    "test:lint": "eslint",
    "build": "bundt src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saibotsivad/ragu.git"
  },
  "keywords": [
    "markdown",
    "noddity",
    "ssg",
    "11ty"
  ],
  "author": {
    "name": "Tobias Davis",
    "email": "tobias@davistobias.com",
    "url": "https://davistobias.com"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/saibotsivad/ragu/issues"
  },
  "homepage": "https://github.com/saibotsivad/ragu",
  "optionalDependencies": {
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@saibotsivad/blockdown": "^3.0.0",
    "@saibotsivad/eslint-config-saibotsivad": "^0.0.4",
    "js-yaml": "^4.1.0",
    "snarkdown": "^2.0.0"
  },
  "dependencies": {
    "p-map": "^5.5.0",
    "sade": "^1.8.1"
  }
}
